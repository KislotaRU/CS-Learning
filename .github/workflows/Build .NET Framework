 name: Build .NET Framework

   on:
     push:
       branches:
         - master
     pull_request:
       branches:
         - master

   jobs:
     build:
       runs-on: windows-latest
       steps:
         - name: Checkout code
           uses: actions/checkout@v2

         - name: Setup .NET Framework
           run: |
             choco install dotnetfx -y

         - name: Build
           run: msbuild CS JUNIOR.sln.sln 
